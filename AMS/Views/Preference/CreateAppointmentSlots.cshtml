@model AMS.Models.AppointmentSlots

@{
    ViewBag.Title = "CreateAppointmentSlots";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>CreateAppointmentSlots</h2>

@using (Html.BeginForm()) {
    @Html.ValidationSummary(true)

    <fieldset>
        <legend>AppointmentSlots</legend>
           @if (ViewBag.Message != null)
        {
            <div style="border:solid 2px green">
                @ViewBag.Message
            
            
            </div>
        }
         <div class="editor-label">
            @Html.LabelFor(model => model.numberOfdays)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.numberOfdays)
            @Html.ValidationMessageFor(model => model.numberOfdays)
        </div>
        <p>
            <input type="submit" value="Create" />
        </p>
    </fieldset>
}

<div>
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
