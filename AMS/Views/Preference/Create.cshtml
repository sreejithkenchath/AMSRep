@model AMS.UserPreference

@{
    ViewBag.Title = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Create</h2>

@using (Html.BeginForm()) {
    @Html.ValidationSummary(true)

    <fieldset>
        <legend>UserPreference</legend>

       
        <div class="editor-label">
            @Html.LabelFor(model => model.BookingDays)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.BookingDays)
            @Html.ValidationMessageFor(model => model.BookingDays)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.SlotDuration)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.SlotDuration)
            @Html.ValidationMessageFor(model => model.SlotDuration)
        </div>

        <p>
            <input type="submit" value="Next" />
        </p>
    </fieldset>
}

